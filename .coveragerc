[run]
source = 
    complaint_search

omit = 
    */wsgi.py
    *admin*
    *migrations/*
    *templatetags/*
    */site-packages*
    */python2.7/*
    *setup.py
    *manage.py
    *test*
    *__init__.py
    *settings.py
    *urls.py

[report]
exclude_lines = 
    pragma: no cover
    if __name__ == .__main__.:
    datetime.now()